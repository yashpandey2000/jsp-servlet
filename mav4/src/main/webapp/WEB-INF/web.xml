  	<servlet-class>co.in.control.registration</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>regiscontrol</servlet-name>
  	<url-pattern>/regiscontrol</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>register</servlet-name>
  	<url-pattern>/register</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>login</servlet-name>
  	<url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
